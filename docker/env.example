
# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=username
DB_PASSWORD=password
DB_NAME=database_name

# API keys (replace with real keys)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
GCP_PROJECT_ID=your_gcp_project_id
GCP_SERVICE_ACCOUNT_KEY_PATH=/path/to/your/gcp_service_account.json

# Path configurations
DATA_PATH=/app/data/raw               # Path for raw data files
PROCESSED_DATA_PATH=/app/data/processed # Path for processed data files
MODEL_PATH=/app/models                 # Path where trained models are stored
LOGS_PATH=/app/logs                    # Path for storing logs

# Model configuration
MODEL_TYPE=resnet50                    # Type of model (e.g., resnet50, bert, lstm)
BATCH_SIZE=32                          # Batch size for training
LEARNING_RATE=0.001                    # Learning rate for training
EPOCHS=100                             # Number of training epochs

# External API configuration (e.g., for external data sources)
EXTERNAL_API_URL=https://api.example.com
EXTERNAL_API_KEY=your_api_key_here

# Debugging and logging
LOG_LEVEL=INFO                         # Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
DEBUG_MODE=False                       # Enable debug mode (True/False)

# Miscellaneous
RANDOM_SEED=42                         # Seed for reproducibility
